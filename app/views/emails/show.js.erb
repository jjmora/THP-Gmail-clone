function show_email(){
  box = document.getElementById("content")
  while (box.firstChild) {
    box.removeChild(box.firstChild);
  }
  document.getElementById("content").insertAdjacentHTML("afterbegin", "<%= j render 'email_list', email: @email %>")


  list_element = document.getElementById("list-<%= @email.id %>").classList
  // .toggle('unread')
  console.log(list_element)
  if(list_element.classList == 'unread'){
    console.log('unread')
  } else {
    console.log('read')
  }

}
show_email()